<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane fx:id="myProfilePane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            onMouseEntered="#onMouseEntered" prefHeight="470.0" prefWidth="664.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="darya.risks.client.controller.MyProfileController">
    <children>
        <Button fx:id="saveChangesBtn" disable="true" layoutX="210.0" layoutY="159.0" mnemonicParsing="false"
                onAction="#saveChanges" text="Save Changes"/>
        <VBox layoutX="190.0" layoutY="28.0" prefHeight="112.0" prefWidth="219.0" spacing="4.0">
            <children>
                <TextField fx:id="myFirstNameTextField" prefHeight="25.0" prefWidth="79.0" promptText="First Name"/>
                <TextField fx:id="myLastNameTextField" prefHeight="25.0" prefWidth="79.0" promptText="Last Name"/>
                <TextField fx:id="myEmailTextField" disable="true" prefHeight="25.0" prefWidth="79.0"
                           promptText="Email"/>
                <TextField fx:id="myPasswordTextField" promptText="password"/>
            </children>
        </VBox>
        <VBox layoutX="66.0" layoutY="28.0" prefHeight="104.0" prefWidth="110.0" spacing="12.0">
            <children>
                <Label text="First Name"/>
                <Label text="Last Name"/>
                <Label text="Email"/>
                <Label text="Password"/>
            </children>
        </VBox>
    </children>
</AnchorPane>
